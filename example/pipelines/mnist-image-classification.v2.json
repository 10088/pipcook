{
  "specVersion": "2.0",
  "from": "https://cdn.jsdelivr.net/gh/imgcook/dataset@main/cv/mnist.mjs",
  "dataflow": [
    "https://cdn.jsdelivr.net/gh/imgcook/pipcook-scripts@main/formats/pascalvoc.mjs",
    "https://cdn.jsdelivr.net/gh/imgcook/pipcook-scripts@main/processors/resize-image.mjs?size=224",
    "https://cdn.jsdelivr.net/gh/imgcook/pipcook-scripts@main/processors/normalize.mjs"
  ],
  "model": "https://cdn.jsdelivr.net/gh/imgcook/pipcook-models@main/cv/mobilenet.mjs",
  "artifacts": [{
    "type": "local",
    "path": "./file/to/your/model/dir"
  }],
  "options": {
    "framework": "^tensorflow@2",
    "train": {
      "epochs": 20,
      "validationRequired": true
    }
  }
}
